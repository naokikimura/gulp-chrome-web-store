{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "curly": [
      true,
      "ignore-same-line"
    ],
    "max-classes-per-file": [
      true,
      5,
      "exclude-class-expressions"
    ]
  },
  "rulesDirectory": []
}